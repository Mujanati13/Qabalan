# Environment Configuration
NODE_ENV=development

# Backend Configuration
VITE_API_URL=http://localhost:3015/api
VITE_SOCKET_URL=http://localhost:3015
VITE_BACKEND_BASE_URL=http://localhost:3015

# Frontend URLs
VITE_WEB_CLIENT_URL=http://localhost:3070
VITE_ADMIN_DASHBOARD_URL=http://localhost:3006

# App Configuration
VITE_APP_NAME=FECS Web Client
