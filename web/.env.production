# Production Environment Configuration
NODE_ENV=production

# Backend Configuration - Using your actual domains
VITE_API_URL=https://qablanapi.albech.me/api
VITE_SOCKET_URL=https://qablanapi.albech.me
VITE_BACKEND_BASE_URL=https://qablanapi.albech.me

# Frontend URLs
VITE_WEB_CLIENT_URL=https://qablan.albech.me
VITE_ADMIN_DASHBOARD_URL=https://qablan.albech.me

# App Configuration
VITE_APP_NAME=FECS Web Client
VITE_APP_VERSION=1.0.0

# Development settings
VITE_DEBUG=false
VITE_LOG_LEVEL=error

# Features
VITE_ENABLE_PWA=true
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_NOTIFICATIONS=true

# External services
VITE_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here
VITE_STRIPE_PUBLIC_KEY=your_stripe_public_key_here

# Image optimization
VITE_IMAGE_OPTIMIZATION=true
VITE_IMAGE_QUALITY=80
VITE_IMAGE_FORMATS=webp,avif,jpeg

# Performance
VITE_ENABLE_COMPRESSION=true
VITE_ENABLE_CACHING=true

# Security
VITE_ENABLE_SSL=true
VITE_SECURE_COOKIES=true

# Socket.IO Configuration
VITE_SOCKET_TRANSPORTS=websocket,polling
VITE_SOCKET_TIMEOUT=20000
VITE_SOCKET_RECONNECTION=true
