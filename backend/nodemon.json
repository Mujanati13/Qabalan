{
  "watch": [
    "*.js",
    "routes/**/*.js",
    "middleware/**/*.js",
    "models/**/*.js",
    "services/**/*.js",
    ".env"
  ],
  "ext": "js,json,env",
  "ignore": [
    "node_modules/**",
    "test/**",
    "*.test.js"
  ],
  "env": {
    "NODE_ENV": "development"
  }
}
